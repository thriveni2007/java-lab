class Outer{
    void outermethod(){
    class Inner{
        void display(){
            System.out.println("this is the inner class.");
        }
    }
        Inner i=new Inner();
        i.display();
    }
}
class Main {
    public static void main(String[] args) {
        Outer o =new Outer();
        o.outermethod();
    }
}
